    "bootstrap": "^3.3.6",
    "less": "^2.5.3",
    "livereload": "^0.4.0",
    "serve": "^1.4.0",
    "ui-toolkit": "holidayextras/ui-toolkit#0.26.4",
    "watchy": "^0.6.5"
